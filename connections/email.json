{
  "name": "email",
  "strategy": "email",
  "is_domain_connection": false,
  "enabled_clients": ["Auth0 Demo SPA", "Auth0 Demo Web App"],
  "options": {
    "name": "email",
    "disable_signup": false,
    "email": {
      "body": "./email.html",
      "from": "{{ application.name }} <rdahbura@gmail.com>",
      "syntax": "liquid",
      "subject": "Welcome to {{ application.name }}"
    },
    "totp": {
      "length": 6,
      "time_step": 300
    },
    "brute_force_protection": true
  }
}
